    "express-handlebars": "^8.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bun": "latest",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.1"
  },
  "private": true
